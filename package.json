{
    "name": "multiplayer-game",
    "version": "1.0.0",
    "description": "Simple multiplayer using socket.io, phaser and typescript",
    "main": "index.js",
    "scripts": {
        "dev": "VITE_DEV_MODE=true vite",
        "dev:production": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "server": "tsx server/index.ts",
        "server:watch": "tsx watch server/index.ts",
        "lint": "eslint . --ext .ts,.tsx",
        "lint:fix": "eslint . --ext .ts,.tsx --fix",
        "format": "prettier --write \"**/*.{ts,tsx,json,css,md}\"",
        "precommit": "npm run lint"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tomcoolnl2/phaser"
    },
    "author": "",
    "license": "MIT",
    "homepage": "https://github.com/tomcoolnl2/phaser#readme",
    "dependencies": {
        "express": "4.21.1",
        "phaser": "3.87.0",
        "socket.io": "4.8.1",
        "socket.io-client": "4.8.1",
        "uuid": "11.0.3"
    },
    "devDependencies": {
        "@types/express": "4.17.21",
        "@types/node": "22.9.0",
        "@types/uuid": "10.0.0",
        "@typescript-eslint/eslint-plugin": "8.13.0",
        "@typescript-eslint/parser": "8.13.0",
        "@vitejs/plugin-basic-ssl": "1.1.0",
        "eslint": "9.14.0",
        "eslint-config-prettier": "9.1.0",
        "husky": "9.1.6",
        "nodemon": "3.1.7",
        "prettier": "3.3.3",
        "tsx": "4.19.2",
        "typescript": "5.6.3",
        "vite": "5.4.11"
    }
}
